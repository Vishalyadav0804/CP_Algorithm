
In O(n^3)

#include <iostream>
using namespace std;
int main() 
{
    int n,i,j,k;
    cin>>n;
    int *a=new int[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<n;i++)
    {
        for(j=i;j<n;j++)
        {
            for(k=i;k<=j;k++)
            {
                cout<<a[k]<<" ";
            }
            cout<<endl;
        }
    }
}


In O(n^2)

for(i=0;i<n;i++)
    {
        string str="";
        for(j=i;j<n;j++)
        {
            str=str+s[j];
            cout<<str<<endl;
        }
    }

